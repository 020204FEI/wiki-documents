{
  "index_name": "wiki",
  "start_urls": ["https://wiki.seeedstudio.com"],
  "stop_urls": [],
  "sitemap_urls": ["https://wiki.seeedstudio.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "selectors": {
      "lvl0": ".sidebar-heading.active",
			"lvl1": "h1",
			"lvl2": "h2",
			"lvl3": "h3",
			"lvl4": "h4",
			"lvl5": "h5",
			"lvl6": "h6",
			"text": "p, li",
    	"lang": {
				"selector": "/html/@lang",
				"type": "xpath",
				"global": true,
				"default_value": "zh-CN"
			}
  }
      "indexHeadings": true,
      "scrape_start_urls": false,
    	"strip_chars": " .,;:#",
    	"custom_settings": {
		"attributesForFaceting": [
			"lang"
		]
	}
}
